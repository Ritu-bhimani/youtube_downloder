        // Handle browser back button click
        function goBackAndReloadHomePage() {
            // First, go back to the previous page in the history
            window.history.back();

            // After going back, check if the current page is the home page
            if (window.location.pathname === '/') {
                // If the current page is the home page, then reload it
                window.location.reload();
            } else {
                // If the current page is not the home page, then navigate to the home page and reload it
                window.location.href = '/';
            }
        }

